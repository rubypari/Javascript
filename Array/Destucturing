let arr=[2,8,9,29];
let [a,b,c,d]=arr;
console.log(arr);

///  2) code

function test(){
   return [6,98,78,34];
}
let [a,b,c,d]= test();
console.log(a,b,c,d);

/// 3) code
let arr=[2,45,76,98];
let [a,,c,d]=arr;
console.log(a,c,d);


///  4)  code

let a=4;
let b=5;
[a,b]=[b,a]
console.log(a,b);

///  5) code

let arr=[2,5,7,3];
let [a,...b]=arr;
console.log(a,b);

/// 6)  code

let arr=[4];
let [a,b=0]=arr;
console.log(a,b);


