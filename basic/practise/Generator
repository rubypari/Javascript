// 10) code

function* gen(){
   try{
     yield "One";
     yield "Two";
   }catch(err){
     console.log("Error:"+err);
   }
}
 const gObj= gen();
 console.log(gObj.next());
  console.log(gObj.throw());

// 11) code
function* gen(){
  let a=yield "first";
  console.log(a);
  a=yield "second";
  console.log(a);
}
const gObj= gen();
gObj.next("A");
gObj.next("B");
gObj.next("C");
